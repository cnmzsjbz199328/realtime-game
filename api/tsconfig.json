{
    "extends": "../tsconfig.json",
    "compilerOptions": {
        "module": "ESNext",
        "moduleResolution": "bundler",
        "target": "ES2022",
        "lib": [
            "ES2022"
        ],
        "noEmit": true,
        "isolatedModules": true,
        "skipLibCheck": true
    },
    "include": [
        "**/*.ts"
    ]
}